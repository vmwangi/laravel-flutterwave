{
    "name": "vmwangi/laravel-flutterwave",
    "description": "A Flutterwave's Version 3 Implementation For Laravel",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "keywords": ["rave", "flutterwave", "payment", "transfers", "africa", "laravel"],
    "homepage": "https://github.com/vmwangi/laravel-flutterwave",
    "autoload": {
        "classmap": [
            "src/",
            "tests/"
        ],
        "psr-4": {
            "Laravel\\Flutterwave\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^9.0|^10.1",
        "mashape/unirest-php": "^3.0|^4.0" 
   },
   "require-dev": {
        "phpunit/phpunit" : "^9.6|^10.1",
        "scrutinizer/ocular": "^1.1",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^8.5",
        "friendsofphp/php-cs-fixer": "^3.17",
        "satooshi/php-coveralls": "^0.1.0",
        "php-coveralls/php-coveralls": "^0.1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Flutterwave\\RaveServiceProvider"
            ],
            "aliases": {
                "Rave": "Laravel\\Flutterwave\\Facades\\Rave"
            }
        }
    },
    "authors": [
        {
            "name": "Flutterwave Developers",
            "email": "developers@flutterwavego.com"
        },
        {
            "name": "Valentine ",
            "email": "valentine@afterwork.ai"
        }
    ]
}
